<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_third"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/a"
    tools:context="com.example.administrator.shushu1.Third">

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/b"
        android:background="#00000000"
        android:id="@+id/imageButton10"
        android:layout_alignTop="@+id/imageButton9"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text="主页"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView10"
        android:textColor="#fffefe"
        android:gravity="center"
        android:layout_below="@+id/imageButton10"
        android:layout_alignStart="@+id/imageButton9"
        android:layout_alignEnd="@+id/imageButton9" />

    <TextView
        android:text="我的店铺"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView11"
        android:textColor="#fffefe"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text="我的信息"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView12"
        android:textColor="#fffefe"
        android:layout_below="@+id/imageButton11"
        android:layout_alignStart="@+id/imageButton11" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/b"
        android:background="#00000000"
        android:layout_marginStart="16dp"
        android:id="@+id/imageButton9"
        android:layout_above="@+id/textView11"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/b"
        android:background="#00000000"
        android:id="@+id/imageButton11"
        android:layout_marginEnd="14dp"
        android:layout_above="@+id/textView11"
        android:layout_alignParentEnd="true" />

    <TextView
        android:text="个人主页"
        android:gravity="center"
        android:textSize="30sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView5"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="11dp" />


    <Button
        android:text="关于我们"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:layout_marginTop="31dp"
        android:id="@+id/button"
        android:layout_below="@+id/textView5"
        android:layout_alignParentStart="true" />

    <Button
        android:text="用户隐私与协议"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:id="@+id/button3"
        android:layout_below="@+id/button2"
        android:layout_centerVertical="true"
        android:layout_alignParentStart="true" />
    <Button
        android:text="设置"
        android:layout_width="wrap_content"
        android:layout_height="70dp"
        android:id="@+id/buttonshe"
        android:layout_below="@+id/button3"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true" />
    <Button
        android:text="用户反馈"
        android:layout_width="wrap_content"
        android:layout_height="70dp"
        android:id="@+id/buttonfan"
        android:layout_below="@+id/buttonshe"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:text="注销"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:id="@+id/button4"
        android:layout_marginBottom="12dp"
        android:layout_below="@+id/buttonfan"
        android:layout_alignParentStart="true" />

    <Button
        android:text="修改密码"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:id="@+id/button2"
        android:layout_below="@+id/button"
        android:layout_alignParentStart="true" />


</RelativeLayout>
